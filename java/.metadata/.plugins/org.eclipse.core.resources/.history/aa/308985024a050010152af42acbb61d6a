package Ch31;

class Person{
	private String name;
	private int age;
	// 생성자(디폴트, 모든 인자)
	Person() {};
	
	public Person(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	
}


public class C01Lamda {
	public static void main(String[] args) {
		
		// 람다식
		// ()->{}
		// 람다 스트림 : 물줄기가 흐르듯 식을 이어붙여 다양한 기능 사용
		// ()->{}.stream()
		
		
	}
}
