package Ch34;

// 어노테이션 구현할 메인

@CustomAnnotation(value="HelloWorld",number=5)
class Simple{
	String v1;
	int v2;
	
	Simple(){
		// reflection 이용, 객체가 생성된 시점에 받아옴??
		CustomAnnotation ref = this.getClass().getAnnotation(CustomAnnotation.class);
		
	}
	
	// toString
	@Override
	public String toString() {
		return "Simple [v1=" + v1 + ", v2=" + v2 + "]";
	};
}

public class Main {

}
