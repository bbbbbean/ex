package Ch35;

public class Main {
	public static void main(String[] args) {
		
		Marine marine1 = new Marine();
		marine1.setGun(new Pistol());
		
		Marine marine2 = new Marine();
		marine2.setGun(new Rifle());
		
		// m1 -> m2
		new Thread() {
			@Override
			public void run() {
			}
		};
		
		// m2 -> m1
		new Thread() {
			@Override
			public void run() {
			}
		};
		
		
	}
}
