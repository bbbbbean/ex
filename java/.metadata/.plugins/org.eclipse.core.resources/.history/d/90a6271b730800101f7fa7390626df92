package Ch35;

import Ch35.building.Barrack;

public class Main2 {
	// 배럭 건물
	static Barrack barrack = new Barrack();
	public static void main(String[] args) {
		
		
		// 건물생성
		new Thread() {

			@Override
			public void run() {
				barrack 
				barrack.BuildStructure();
			}
		}.start();
		
		
		
	}
}
