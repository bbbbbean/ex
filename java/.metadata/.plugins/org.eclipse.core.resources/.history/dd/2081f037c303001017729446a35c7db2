package Ch23;

// 2가지 방식
// run 인터페이스
// thread 클래스

public class C01ThreadMain {
	public static void main(String[] args) {
		
		for(int i=0;i<5;i++) {
			System.out.println("task 01");
			Thread.sleep(500);
		}
		
		for(int i=0;i<5;i++) {
			System.out.println("task 02");
			Thread.sleep(500);
		}
		
		
	}
}
