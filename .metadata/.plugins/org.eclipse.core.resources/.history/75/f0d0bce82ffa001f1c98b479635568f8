package Ch11;

class Company{
	
	// 싱글톤 패턴 구조
	private static Company instance;	// !중요
	private Company() {}	// !중요
	public Company getInstance() {	// !중요
		if(instance==null) {
			instance = new Company();
		}
		return instance;
	}
	
	
}



public class C03SingletonPattern {
	public static void main(String[] args) {
		
	}
}
