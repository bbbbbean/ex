//		}
		if(m>n) {
			int tmp=m;
			m = n;
			n = tmp;
		}
		System.out.println(n);
		System.out.println(m);
		// 구구단 시작
		int dan = n;
		int i=1;
		while(dan<=m) {
			System.out.println(dan+"단");
			dan++;
		}
		
		
		
		
