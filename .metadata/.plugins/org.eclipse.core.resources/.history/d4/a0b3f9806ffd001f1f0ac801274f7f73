package Ch15;

public class C01NullpointerExceptionMain {
	public static void main(String[] args) {
		
		String str = null;
		System.out.println(str.toString());
		System.out.println("Hello Wolrd");
		System.out.println("Hello Wolrd");
		// 예외 처리 전 실행 시
		// Cannot invoke "String.toString()" because "str" is null

		
	}
}
