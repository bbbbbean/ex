package Ch07;

public class C01While {
	public static void main(String[] args) {
		
		// 반복문
		// 조건식을 전제로 함
		
		// 10번 반복 - Hello World
		// 탈출용 변수 : i = 0
		// 특정 조건 만족 시 반복 탈출, 탈출용 조건식 : i<10
		// 탈출용 연산식 : i++
		
//		int i=0;
//		while(조건식) {
//			반복 코드
//			
//			탈출용 연산식(i++, i--)
//		}
		int i=0;
		while (i<10) {
			System.out.println("Hello World");
			i++;
		}
		
		
		
		
		
		
		
		
	}
}
