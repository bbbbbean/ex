package Ch14;

class A{
	
}


public class C01ObjectMain {
	public static void main(String[] args) {
		A ob1 = new A();
		System.out.println(ob1);
		// getclass : reflection기반?
		// notify, notifyAll : 스레드 관련 처리, 비동기 처리를 위한 우선순위 선정을 위해 사용
		
		Object ob2 = new Object();
		System.out.println(ob2);

	}
}
