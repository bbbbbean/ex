package Ch07;

public class C02While {
	public static void main(String[] args) {
		
		// 2단~9단 출력
		int dan = 2;
		while(dan<10) {
			int i=1;
			while(i<10) {
				System.out.printf("%d X %d = %d\n",dan,i,dan*i);
				i++;
			}
			dan++;
			System.out.println();
		}
		
		// 2단~9단 역순 출력 (2x9, 2x8, 2x7 ... )

		
		// 9단~2단 출력

		
		// 2단~N단 출력
	}
}
 