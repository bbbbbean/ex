package Ch35;

public class Medic extends Unit {

	private int healingPoint;
	
	public Medic() {
		hp=100;
		amor=100;
	}
	void setType(String type) {
		this.type = type;
	}
	public void Healling(Unit unit) {
		unit.hp+=healingPoint;
	}
	
	@Override
	public void move() {
		
	}

	@Override
	public void UnderAttack(int damage) {
		
	}

}
