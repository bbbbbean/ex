package Ch19;

public class Snippet {
	public static void main(String[] args) {
		System.out.println("진행중");
				while(true) {
					int data = in.read();
					if(data==-1)break;
					out.write((byte)data); 	// 공간 낭비 방지를 위해 byte화
					out.flush();
				}
				in.close();
				out.close();
				System.out.println("진행완료");
	}
}

