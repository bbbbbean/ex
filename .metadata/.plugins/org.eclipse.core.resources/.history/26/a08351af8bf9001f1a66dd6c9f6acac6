package Ch10;

import java.util.Scanner;

public class C01ArrayMain {
	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		
		int [] arr1 = new int[5];
		System.out.println("5개의 값을 입력하세요");
		arr1[0] = sc.nextInt();
		arr1[1] = sc.nextInt();
		arr1[2] = sc.nextInt();
		arr1[3] = sc.nextInt();
		arr1[4] = sc.nextInt();
		
		
		
	}
}
