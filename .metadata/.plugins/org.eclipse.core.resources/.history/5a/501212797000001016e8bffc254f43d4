package Ch19;

import org.jsoup.Connection;
import org.jsoup.Jsoup;

public class C10JsoupMain {
	public static void main(String[] args) {
		// 이미지 긁어오기?
		// Jsoup 외부 라이브러리
		
		// URL 연결
		Connection conn = Jsoup.connect("https://www.behance.net/");
		// 문서를 object형태로 들고옴
		Document dom = conn.get();
		
		
		
	}
}
