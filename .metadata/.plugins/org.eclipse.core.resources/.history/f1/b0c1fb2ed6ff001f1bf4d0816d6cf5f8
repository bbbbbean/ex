public class C06FileOutputStreamMain {
	public static void main(String[] args) throws Exception {
		
		OutputStream out = new FileOutputStream("C:\\IOTEST\\test4.txt");
		out.write('a');
		out.write('b');
		out.write('c');
		
		
	}
}
